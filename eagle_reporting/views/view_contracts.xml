<?xml version="1.0"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="eagle_reporting_view_contract_form">
            <field name="name">Eagle Reporting Contract inherit View</field>
            <field name="model">eagle.contract</field>
            <field name="inherit_id" ref="eagle_project.eagle_project_view_contract_form"/>
            <field name="priority" eval="17"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='sale_subscription_line']/tree/field[@name='recurring_next_date']" position="after">
                    <field name="page_break"/>
                </xpath>
                <xpath expr="//field[@name='current_sale_order_lines']/tree/field[@name='requested_date']" position="before">
                    <field name="page_break"/>
                </xpath>
            </field>
        </record>
        <record model="ir.ui.view" id="eagle_reporting_view_contract_form2">
            <field name="name">Eagle Reporting Contract inherit View 2</field>
            <field name="model">eagle.contract</field>
            <field name="inherit_id" ref="eagle_base.eagle_view_contract_form"/>
            <field name="priority" eval="17"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='other_infos']/group/group[@name='various']" position="inside">
                    <field name="do_page_break"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>