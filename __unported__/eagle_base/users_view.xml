<openerp>
	<data>

		<record model="ir.ui.view" id="eagle_view_full_user_inh_01">
			<field name="name">Eagle Full User View Inherit 1</field>
			<field name="model">res.users</field>
			<field name="inherit_id" ref="base.view_users_form"/>
            <field name="priority" eval="17"/>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<field name="menu_id" position="after">
					<field name="eagle_tabs_profile"/>
					<field name="eagle_parm_inline_pos_edit"/>
				</field>
			</field>
		</record>

		<record model="ir.ui.view" id="eagle_view_simple_user_inh_01">
			<field name="name">Eagle Simple User View Inherit 1</field>
			<field name="model">res.users</field>
			<field name="inherit_id" ref="base.view_users_form_simple_modif"/>
            <field name="priority" eval="17"/>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<group name="preferences" position="after">
                    <group name="eagle" string="Eagle" col="4">
    					<field name="eagle_tabs_profile"/>
    					<field name="eagle_parm_inline_pos_edit"/>
                    </group>
				</group>
			</field>
		</record>

    </data>
</openerp>
